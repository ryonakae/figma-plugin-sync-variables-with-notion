{
  "name": "figma-plugin-sync-variables-with-notion",
  "version": "1.0.0",
  "description": "MIT",
  "author": "Ryo Nakae",
  "scripts": {
    "build": "concurrently -P 'npm:build:* -- {*}'",
    "build:css": "npx @tailwindcss/cli -i ./src/ui/styles/input.css -o ./src/ui/styles/output.css",
    "build:js": "build-figma-plugin --typecheck",
    "check": "biome check",
    "prepare": "husky",
    "watch": "npm run build:css && concurrently -P 'npm:build:* -- {*}' -- --watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,json}": ["biome check --apply"]
  },
  "dependencies": {
    "@create-figma-plugin/ui": "4.0.0-alpha.0",
    "@create-figma-plugin/utilities": "4.0.0-alpha.0",
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@tailwindcss/postcss": "4.0.13",
    "@tanstack/react-virtual": "3.13.3",
    "postcss": "8.5.3",
    "preact": "10.26.4",
    "react-use": "17.6.0",
    "tailwind-merge": "3.0.2",
    "tailwindcss": "4.0.13",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@create-figma-plugin/build": "4.0.0-alpha.0",
    "@create-figma-plugin/tsconfig": "4.0.0-alpha.0",
    "@figma/plugin-typings": "1.108.0",
    "@tailwindcss/cli": "4.0.13",
    "@types/node": "22.13.10",
    "concurrently": "9.1.2",
    "dotenv": "16.4.7",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "typescript": "5.8.2"
  },
  "figma-plugin": {
    "editorType": ["figma"],
    "id": "sync-variables-with-notion",
    "name": "Sync Variables with Notion",
    "main": "src/main/index.ts",
    "ui": "src/ui/index.tsx",
    "relaunchButtons": {
      "open": {
        "name": "Sync Variables with Notion",
        "main": "src/main/index.ts",
        "ui": "src/ui/index.tsx"
      }
    },
    "documentAccess": "dynamic-page",
    "permissions": ["teamlibrary"]
  }
}
